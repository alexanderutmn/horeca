<!-- row -->
<div class="container-fluid">
  <div class="row page-titles mx-0">
    <div class="col-sm-6 p-md-0">
      <div class="welcome-text">
        <h4>{{title}}</h4>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xl-12 col-lg-12">
      <div class="card">
        <div class="card-body">
          {{#if jsonDataIs}}
            {{#each jsonDataError}}
              <p>Не получилось импортировать
                {{Название}}
                потому что
                {{Причина}}</p>

            {{/each}}
          {{/if}}
          <div class="basic-form">
            <form method="post">
              <div class="form-group row align-items-center">
                <label class="col-sm-3 col-form-label">Защита от случайной
                  синхронизации</label>
                <div class="col-sm-9">
                  <input
                    type="checkbox"
                    name="enableLoadingDataFromIiko"
                    value="true"
                  />
                </div>
              </div>

              <div class="form-group row">
                <div class="col-sm-10">
                  <button type="submit" class="btn btn-primary">Начать</button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>