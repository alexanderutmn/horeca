<!DOCTYPE html>
<html lang="ru">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link rel="preload" as="image" href="/assets/css/loader.jpeg" />
    <link rel="preload" as="image" href="/assets/images/loader-new.svg" />

    {{#if imgLoad}}
      <meta property="og:image" content="{{{urlSeo}}}/assets/css/loader.jpeg"/>
  		<meta property="og:image:secure_url" content="{{{urlSeo}}}/assets/css/loader.jpeg"/>
    {{else}}
      <meta property="og:image" content="https://easyqr.ru/upload/medialibrary/941/94131a77de3d17f27ad09b79fc795488.png" />
      <meta property="og:image:secure_url" content="https://easyqr.ru/upload/medialibrary/941/94131a77de3d17f27ad09b79fc795488.png" />
    {{/if}}
		<meta property="og:image:type" content="image/png" />
		<meta property="og:image:width" content="200px" />
		<meta property="og:image:height" content="200px" />
		<meta property="og:image:alt" content="Easy QR" />

    <meta property="og:type" content="business.business">
  	<meta property="og:title" content="{{{titleSeo}}}">
  	<meta property="og:description" content="{{{descriptionSeo}}}">
  	<meta property="og:site_name" content="Easy QR">
  	<meta property="og:locale" content="ru_RU">
    <meta property="og:url" content="{{{urlSeo}}}">

    <meta name="description" content="{{{descriptionSeo}}}">
    <meta name="keywords" content="{{{keywordsSeo}}}">
    <link rel="icon" href="/favicon.ico">

    <link rel="stylesheet" href="/assets/css/colors.css?x={{{cssTime}}}" type="text/css">
    <link rel="stylesheet" href="/assets/js/lib/owl/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="/assets/js/lib/owl/owl.theme.default.min.css" type="text/css">
    <link rel="stylesheet" href="/assets/bundles/main.bundle.css" type="text/css">
    <link type="text/css" href="/assets/fonts/stylesheet.css" rel="preload" as="style" onload="this.rel='stylesheet'" media="all">
    <link type="text/css" href="/assets/js/lib/slick/slick.css" rel="preload" as="style" onload="this.rel='stylesheet'" media="all">
    <link type="text/css" href="/assets/js/lib/slick/slick-theme.css" rel="preload" as="style" onload="this.rel='stylesheet'" media="all">

    <title>{{{titleSeo}}}</title>

    {{#if yandexMetrika}}
      <!-- Yandex.Metrika counter -->
      <script type="text/javascript" >
        setTimeout(() => {
          (function(m,e,t,r,i,k,a){m[i]=m[i]||function(){(m[i].a=m[i].a||[]).push(arguments)};
          m[i].l=1*new Date();k=e.createElement(t),a=e.getElementsByTagName(t)[0],k.async=1,k.src=r,a.parentNode.insertBefore(k,a)})
          (window, document, "script", "https://mc.yandex.ru/metrika/tag.js", "ym");

          ym({{yandexMetrika}}, "init", {
               clickmap:true,
               trackLinks:true,
               accurateTrackBounce:true
          });
        }, 500)
      </script>
      <noscript><div><img src="https://mc.yandex.ru/watch/{{yandexMetrika}}" style="position:absolute; left:-9999px;" alt="" /></div></noscript>
      <!-- /Yandex.Metrika counter -->
    {{/if}}

  </head>
  <body>
    <div class="fixed-crossbrowser-background"></div>
    <div class="content {{#if mainPage}} p-top-105 p-bottom-45 {{/if}}">
      {{> main_top_line }}
      {{{ body }}}
      {{> main_slide_menu }}
    </div>
  </body>

  <div id="loader">
    <div class="loader_img main-color-button">
    </div>
  </div>

  <script defer type="text/javascript" src="/assets/js/lib/jquery.js"></script>
  <script defer type="text/javascript" src="/assets/js/lib/jquery.lazy.min.js"></script>
  <script defer type="text/javascript" src="/assets/js/lib/owl/owl.carousel.min.js"></script>
  <script defer type="text/javascript" src="/socket.io/socket.io.js"></script>
  <script defer type="text/javascript" src="/assets/js/lib/cookie.js"></script>
  <script defer type="text/javascript" src="/assets/js/lib/jquery.touchSwipe.min.js"></script>

  <script defer type="text/javascript" src="/assets/js/lib/masonry.pkgd.js"></script>
  <script defer type="text/javascript" src="/assets/js/lib/notify.min.js"></script>
  <script defer type="text/javascript" src="/assets/js/lib/jquery.mask.js"></script>
  <script defer type="text/javascript" src="/assets/js/lib/slick/slick.min.js"></script>
  <script defer type="text/javascript" src="/assets/bundles/main.bundle.js"></script>
  <script defer type="text/javascript" src="/assets/js/lib/sales.js"></script>

  {{#if enablePayment }}
    <script src="https://widget.cloudpayments.ru/bundles/cloudpayments.js"></script>
  {{/if}}

</html>
