<!DOCTYPE html>
<html lang="ru">
   <head>
      <meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta name="viewport" content="width=device-width,initial-scale=1">
      <title>{{{title}}}</title>
      <!-- Favicon icon -->
      <link rel="icon" href="/favicon.ico">
      <!-- Daterange picker -->
      <link href="/assets_admin/vendor/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
      <link href="/assets_admin/vendor/datatables/css/jquery.dataTables.min.css" rel="stylesheet">
      <link rel="stylesheet" href="/assets_admin/vendor/select2/css/select2.min.css">
      <link href="/assets_admin/vendor/bootstrap-select/dist/css/bootstrap-select.min.css" rel="stylesheet">
      <link href="/assets_admin/css/style.css" rel="stylesheet">
      <link href="https://cdn.lineicons.com/2.0/LineIcons.css" rel="stylesheet">
      <link rel="stylesheet" href="/assets_admin/vendor/pickadate/themes/default.css">
      <link rel="stylesheet" href="/assets_admin/vendor/pickadate/themes/default.date.css">
      <link  href="/assets_admin/vendor/cropper/cropper.min.css" rel="stylesheet">
   </head>
   <body>
      <!--*******************
         Preloader start
         ********************-->
      {{> admin_preloader }}
      <!--*******************
         Preloader end
         ********************-->
      <!--**********************************
         Main wrapper start
         ***********************************-->
      <div id="main-wrapper">
         <!--**********************************
            Nav header start
            ***********************************-->
         <div class="nav-header">
            <a href="/admin/" class="brand-logo">
               <img class="logo-abbr" src="/assets_admin/images/qr-code.png" alt="">
               <!-- <img class="logo-abbr" src="/assets_admin/images/logo.png" alt=""> -->
               <!-- <img class="logo-compact" src="/assets_admin/images/logo-text.png" alt=""> -->
               <!-- <img class="brand-title" src="/assets_admin/images/logo-text.png" alt=""> -->
               <div class="brand-title" style="color: #000000;font-size: 1.8rem;">
                  Easy QR
               </div>
            </a>
            <div class="nav-control">
               <div class="hamburger">
                  <span class="line"></span><span class="line"></span><span class="line"></span>
               </div>
            </div>
         </div>
         <!--**********************************
            Nav header end
            ***********************************-->
         <!--**********************************
            Chat box start
            ***********************************-->
         {{> admin_chat_box }}
         <!--**********************************
            Chat box End
            ***********************************-->
         <!--**********************************
            Header start
            ***********************************-->
         {{> admin_header }}
         <!--**********************************
            Header end ti-comment-alt
            ***********************************-->
         <!--**********************************
            Sidebar start
            ***********************************-->
         {{> admin_left_menu }}
         <!--**********************************
            Sidebar end
            ***********************************-->
         <!--**********************************
            Content body start
            ***********************************-->
            <div class="content-body">
               {{#if message.success}}
               <div class="container-fluid">
                  <div class="row page-titles mx-0">
                     <div class="alert alert-success alert-dismissible fade show">
                        <button type="button" class="close h-100" data-dismiss="alert" aria-label="Close"><span><i class="mdi mdi-close"></i></span>
                                    </button>
                                    <strong>Готово!</strong> {{message.text}}
                     </div>
                  </div>
               </div>
               {{/if}}
               {{#if message.error}}
               <div class="container-fluid">
                  <div class="row page-titles mx-0">
                     <div class="alert alert-danger alert-dismissible fade show">
                        <button type="button" class="close h-100" data-dismiss="alert" aria-label="Close"><span><i class="mdi mdi-close"></i></span>
                                    </button>
                                    <strong>Ошибка!</strong> {{message.text}}
                     </div>
                  </div>
               </div>
               {{/if}}

         {{{ body }}}
         </div>
         <!--**********************************
            Content body end
            ***********************************-->
         <!--**********************************
            Footer start
            ***********************************-->
         <div class="footer">
            <div class="copyright">
               <!-- <p>Copyright © Designed &amp; Developed by <a href="http://dexignzone.com/" target="_blank">DexignZone</a> 2020</p> -->
            </div>
         </div>
         <!--**********************************
            Footer end
            ***********************************-->
         <!--**********************************
            Support ticket button start
            ***********************************-->
         <!--**********************************
            Support ticket button end
            ***********************************-->
      </div>
      <!--**********************************
         Main wrapper end
         ***********************************-->
      <!-- Small modal -->
      <!-- <button type="button" class="btn btn-primary mb-2" data-toggle="modal" data-target=".bd-example-modal-sm">Small modal</button> -->
      <div class="modal fade bd-example-modal-sm" tabindex="-1" role="dialog" aria-hidden="true" id="modal-notify">
         <div class="modal-dialog modal-sm">
            <div class="modal-content">
               <div class="modal-header">
                  <h5 class="modal-title"></h5>
                  <button type="button" class="close" data-dismiss="modal"><span>&times;</span>
                  </button>
               </div>
               <div class="modal-body">Кажется, пришли новые уведомления</div>
            </div>
         </div>
      </div>
      <!--**********************************
         Scripts
         ***********************************-->
      <!-- Required vendors -->
      <script src="/assets_admin/vendor/global/global.min.js"></script>
      <script src="/assets_admin/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
      <script src="/assets_admin/js/jquery.lazy.min.js"></script>
      <script src="/assets_admin/js/custom.min.js"></script>
      <script src="/assets_admin/js/deznav-init.js"></script>
      <!-- Datatable -->
      <script src="/assets_admin/vendor/datatables/js/jquery.dataTables.min.js"></script>
      <script src="/assets_admin/vendor/moment/moment.min.js"></script>
      <script src="/assets_admin/vendor/bootstrap-daterangepicker/daterangepicker.js"></script>
      <!-- pickdate -->
      <script src="/assets_admin/vendor/pickadate/picker.js"></script>
      <script src="/assets_admin/vendor/pickadate/picker.time.js"></script>
      <script src="/assets_admin/vendor/pickadate/picker.date.js"></script>
      <script src="/assets_admin/vendor/pickadate/pickadate-init.js"></script>
      <!-- select -->
      <script src="/assets_admin/vendor/select2/js/select2.full.min.js"></script>

      <script src="/assets_admin/vendor/cropper/cropper.min.js"></script>
      <script src="/assets_admin/vendor/cropper/jquery-cropper.min.js"></script>

      <!-- <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/buzz/1.2.1/buzz.min.js"></script> -->
      <script type="text/javascript" src="/socket.io/socket.io.js"></script>
      <script type="text/javascript" src="/assets_admin/js/notify.min.js"></script>
      <script type="text/javascript" src="/assets_admin/js/qrcode.min.js"></script>
      <script type="text/javascript" src="/assets_admin/js/custom.js"></script>
      <script type="text/javascript" src="/assets_admin/js/admin_socket.js"></script>
   </body>
</html>
